{
  "name": "speakeasy",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "npx vite",
    "web:expo": "expo start --web",
    "test": "node scripts/test-backend-connection.js",
    "test:backend": "node scripts/test-backend.js",
    "test:connection": "node scripts/test-backend-connection.js",
    "test:auth": "node scripts/test-auth.js",
    "backend:local": "cd backend && node server-openai.js",
    "backend:deploy": "cd backend && gcloud run deploy speakeasy-backend --source . --region us-central1 --allow-unauthenticated --project=modular-analog-476221-h8",
    "build:web": "vite build",
    "build:web:expo": "expo export --platform web",
    "build:ios": "eas build --platform ios --profile production",
    "build:android": "eas build --platform android --profile production",
    "build:all": "eas build --platform all --profile production",
    "build:testflight": "eas build --platform ios --profile testflight",
    "submit:testflight": "eas submit --platform ios --profile production --latest",
    "testflight": "./build-testflight.sh",
    "deploy:web": "npm run build:web && gcloud run deploy speakeasy-web --source . --use-docker",
    "deploy:backend": "npm run backend:deploy",
    "deploy:local": "./scripts/deploy-local.sh",
    "setup:gcloud": "./scripts/setup-gcloud.sh",
    "preview": "npx vite preview"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-google-signin/google-signin": "^16.0.0",
    "@react-navigation/bottom-tabs": "^7.5.0",
    "@react-navigation/native": "^7.1.18",
    "@react-navigation/stack": "^7.5.0",
    "@stripe/stripe-react-native": "0.50.3",
    "axios": "^1.12.2",
    "expo": "54.0.21",
    "expo-apple-authentication": "^8.0.7",
    "expo-constants": "~18.0.10",
    "expo-crypto": "^15.0.7",
    "expo-notifications": "^0.32.12",
    "expo-speech": "^14.0.7",
    "expo-speech-recognition": "^2.1.5",
    "expo-status-bar": "~3.0.8",
    "firebase": "^12.4.0",
    "firebase-admin": "^13.5.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-purchases": "^9.6.0",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0"
  },
  "private": true,
  "devDependencies": {
    "@vitejs/plugin-react": "^5.1.0",
    "vite": "^7.1.12",
    "vite-plugin-react-native-web": "^2.4.1"
  }
}
